{
  "parser": "babel-eslint",
  "plugins": ["babel", "react", "react-native", "jsx-a11y"],

  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb"],
  "rules": {
      "react/forbid-component-props": 0
  }
}
